<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuinforma.tuinforma">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tuinforma_app_icono"
        android:label="TuInforma"
        android:roundIcon="@mipmap/tuinforma_app_icono"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Secciones.SeccionPortada.NotaPrincipal"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Secciones.SeccionesGenerales.ServiciosGenerales"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Secciones.SeccionesGenerales.AcercaDe"
            android:screenOrientation="portrait" />
    </application>

</manifest>